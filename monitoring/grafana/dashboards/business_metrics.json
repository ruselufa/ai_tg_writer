{
  "id": null,
  "title": "AI TG Writer - Business Metrics",
  "tags": ["ai-tg-writer", "business"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
      {
        "id": 1,
        "title": "User Conversions",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(user_conversions_total[5m]) * 60",
            "legendFormat": "{{from_tariff}} -> {{to_tariff}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Conversions/min"
          }
        ],
        "lines": true,
        "points": false
      },
      {
        "id": 2,
        "title": "Retention Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "user_retention_rate",
            "legendFormat": "Retention {{period}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Retention Rate",
            "max": 1,
            "min": 0
          }
        ],
        "lines": true,
        "points": false
      },
      {
        "id": 3,
        "title": "ARPU (Average Revenue Per User)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "user_arpu",
            "legendFormat": "ARPU {{period}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "ARPU (RUB)"
          }
        ],
        "lines": true,
        "points": false
      },
      {
        "id": 4,
        "title": "Churn Rate",
