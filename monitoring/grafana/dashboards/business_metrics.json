        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "user_churn_rate",
            "legendFormat": "Churn {{period}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Churn Rate",
            "max": 1,
            "min": 0
          }
        ],
        "lines": true,
        "points": false
      },
      {
        "id": 5,
        "title": "Session Duration by Tariff",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(user_session_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile {{user_tariff}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(user_session_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile {{user_tariff}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)"
          }
        ],
        "lines": true,
        "points": false
      },
